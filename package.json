{
  "name": "atlasform-config-engine",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:ui": "turbo run dev --parallel --filter=@lowcode/admin --filter=@lowcode/web",
    "dev:server": "pnpm --filter @lowcode/server dev",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "proto:gen": "pnpm --filter @lowcode/proto-core proto:gen"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "mongoose": "^8.12.1",
    "turbo": "^2.8.10",
    "typescript": "^5.7.3"
  }
}
